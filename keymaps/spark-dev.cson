'.platform-darwin':
  'cmd-u': 'spark-dev:flash-cloud'
  'cmd-r': 'spark-dev:compile-cloud'
  'cmd-shift-m': 'spark-dev:show-serial-monitor'

'.platform-win32, .platform-linux':
  'ctrl-u': 'spark-dev:flash-cloud'
  'ctrl-r': 'spark-dev:compile-cloud'
  'ctrl-shift-m': 'spark-dev:show-serial-monitor'
